{
    "compilerOptions": {
        "lib": ["ES2022", "DOM", "DOM.Iterable"],
        "esModuleInterop": true,
        "moduleResolution": "NodeNext",
        "module": "NodeNext",
        "target": "ES2022",
        "strict": true,
        "skipLibCheck": true,
        "declaration": true,
        "jsx": "react",
        "noUncheckedIndexedAccess": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noPropertyAccessFromIndexSignature": true
    }
}
