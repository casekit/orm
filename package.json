{
    "name": "@casekit/orm",
    "version": "0.0.1-alpha.14",
    "description": "A simple ORM",
    "type": "module",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "exports": {
        ".": "./lib/index.js"
    },
    "files": [
        "./lib/*",
        "!./lib/**.test.js",
        "!./lib/**.test.js.map",
        "!./lib/**.test.d.ts",
        "!./lib/**.test.d.ts.map"
    ],
    "scripts": {
        "build": "rm -rf lib && tsc",
        "check:all": "CI=true pnpm test && CI=true pnpm lint && CI=true pnpm prettier && CI=true pnpm typecheck",
        "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
        "prettier": "prettier -c src",
        "prettier:write": "prettier -w src",
        "release": "pnpm build && pnpm version --prerelease --preid=alpha && pnpm publish",
        "test": "vitest --typecheck",
        "test:coverage": "vitest --typecheck --coverage",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@anatine/zod-mock": "^3.13.4",
        "@casekit/unindent": "^1.0.3",
        "@faker-js/faker": "^8.4.1",
        "@fast-check/vitest": "^0.1.0",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/lodash-es": "^4.17.12",
        "@types/object-hash": "^3.0.6",
        "@types/pg": "^8.11.5",
        "@types/pg-format": "^1.0.5",
        "@types/pluralize": "^0.0.33",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@vitest/coverage-istanbul": "^1.4.0",
        "@vitest/coverage-v8": "^1.4.0",
        "@vitest/ui": "^1.4.0",
        "dotenv": "^16.4.5",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "fast-check": "^3.17.1",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.4.0"
    },
    "dependencies": {
        "lodash-es": "^4.17.21",
        "object-hash": "^3.0.0",
        "pg": "^8.11.5",
        "pg-format": "^1.0.4",
        "pino": "^9.0.0",
        "pluralize": "^8.0.0",
        "prettier": "^3.2.5",
        "ts-toolbelt": "^9.6.0",
        "uuid": "^9.0.1",
        "zod": "^3.22.4"
    }
}
